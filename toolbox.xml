<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox" style="display: none">
  <category id="http" name="HTTPS" colour="#e65722" icon="wifi">
    <block type="yolobit_http_connect_wifi">
      <value name="WIFI">
        <shadow type="text">
          <field name="TEXT">wifi</field>
        </shadow>
      </value>
      <value name="PASSWORD">
        <shadow type="text">
          <field name="TEXT">password</field>
        </shadow>
      </value>
    </block>

    <block type="yolobit_http_get_request">
      <value name="url">
        <shadow type="text">
          <field name="TEXT">https://jsonplaceholder.typicode.com/users</field>
        </shadow>
      </value>
      <value name="data">
        <shadow type="text">
          <field name="TEXT"></field>
        </shadow>
      </value>
      <value name="header">
        <block type="lists_create_with">
            <mutation items="1"></mutation>
            <value name="ADD0">
              <block type="text">
                <field name="TEXT">User-Agent: OhStem</field>
              </block>
            </value>
        </block>
      </value>
    </block>

    <block type="yolobit_http_get_status_code" />
    <block type="yolobit_http_get_data_text" />
    <block type="yolobit_http_is_ok" />

  </category>
</xml>